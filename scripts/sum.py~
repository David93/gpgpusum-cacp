#!/usr/bin/python
import re
def find_cycle(contents):
    cycle_str = re.compile('gpu_sim_cycle = [\d]+').findall(contents)
    cycles = [int(re.compile('[\d]+').findall(x)[0]) for x in cycle_str]
    return sum(cycles)

import sys
f=open(sys.argv[1])
contents=f.read()
f.close()
print find_cycle(contents)
